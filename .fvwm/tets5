WindowTitle {tets5}
WindowSize  210 5
WindowPosition 300 500
Colorset 28

Font "xft:Noto Sans cjk kr:style=Bold:size=14" 

Init
 Begin
  Set $test = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk0 = (GetOutput {echo } 1 -1)
  Set $Desk1 = (GetOutput {echo } 1 -1)
  Set $Desk2 = (GetOutput {echo } 1 -1)
  Set $Desk3 = (GetOutput {echo } 1 -1)
  Set $Desk4 = (GetOutput {echo } 1 -1)
  Set $Desk5 = (GetOutput {echo } 1 -1)
  Set $Desk6 = (GetOutput {echo } 1 -1)
  Set $homeDir = (GetOutput {echo $HOME} 1 -1) {/.fvwm/}
  Set $w0 = $homeDir {pixmaps/bubbles/w0.png}
  Set $aw0 = $homeDir {pixmaps/bubbles/m.png}

  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk00 = (GetOutput {echo 0} 1 -1)
  If $Deskcurrent == $Desk00 Then
  Begin
    ChangeColorset 1 11  
  End
  Else
  Begin
    ChangeColorset 1 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk11 = (GetOutput {echo 1} 1 -1)
  If $Deskcurrent == $Desk11 Then
  Begin
    ChangeColorset 2 11
  End
  Else
  Begin
    ChangeColorset 2 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk22 = (GetOutput {echo 2} 1 -1)
    If $Deskcurrent == $Desk22 Then
  Begin
    ChangeColorset 3 11  
  End
  Else
  Begin
    ChangeColorset 3 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk33 = (GetOutput {echo 3} 1 -1)
    If $Deskcurrent == $Desk33 Then
  Begin
    ChangeColorset 4 11  
  End
  Else
  Begin
    ChangeColorset 4 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk44 = (GetOutput {echo 4} 1 -1)
    If $Deskcurrent == $Desk44 Then
  Begin
    ChangeColorset 5 11  
  End
  Else
  Begin
    ChangeColorset 5 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk55 = (GetOutput {echo 5} 1 -1)
    If $Deskcurrent == $Desk55 Then
  Begin
    ChangeColorset 6 11
 
  End
  Else
  Begin
    ChangeColorset 6 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk66 = (GetOutput {echo 6} 1 -1)
    If $Deskcurrent == $Desk66 Then
  Begin
    ChangeColorset 7 11  
  End
  Else
  Begin
    ChangeColorset 7 28  
  End  

End

PeriodicTasks
## First Page
Begin
 If (RemainderOfDiv (GetTime) 2)==0 Then
 Begin
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
  Set $Desk00 = (GetOutput {echo 0} 1 -1)
  If $Deskcurrent == $Desk00 Then
  Begin
    ChangeColorset 1 11  
  End
  Else
  Begin
    ChangeColorset 1 28  
  End  
  Set $Deskcurrent = (GetOutput {exec xdotool get_desktop} 1 -1)
