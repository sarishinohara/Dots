WindowTitle {workspace}
WindowSize  210 50
Font "xft:Noto Sans cjk kr:style=Bold:size=14" 

Init
 Begin
  Set $Desk0 = (GetOutput {echo 0} 1 -1)
  Set $Desk1 = (GetOutput {echo 1} 1 -1)
  Set $Desk2 = (GetOutput {echo 2} 1 -1)
  Set $Desk3 = (GetOutput {echo 3} 1 -1)
  Set $Desk4 = (GetOutput {echo 4} 1 -1)
  Set $Desk5 = (GetOutput {echo 5} 1 -1)
  Set $homeDir = (GetOutput {echo $HOME} 1 -1) {/.fvwm/}
  Set $w0 = $homeDir {pixmaps/bubbles/w0.png}
  Set $aw0 = $homeDir {pixmaps/bubbles/aw0.png}


  ChangeTitle 2 $Desk0
  ChangeTitle 4 $Desk1
  ChangeTitle 6 $Desk2
  ChangeTitle 8 $Desk3
  ChangeTitle 10 $Desk4

End

PeriodicTasks
## First Page
Begin
 If (RemainderOfDiv (GetTime) 1)==0 Then
 Begin
  Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
  If $Deskcurrent == 0 Then
  Begin
    Set $Desk = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    ChangeTitle 2 $Deskcurrent
    ChangeIcon 1 $aw0
    ChangeIcon 3 $w0
    ChangeIcon 5 $w0
    ChangeIcon 7 $w0
    ChangeIcon 9 $w0
    ChangeColorset 2 15
    ChangeColorset 4 28 
    ChangeColorset 6 28   
    ChangeColorset 8 28   
    ChangeColorset 10 28   
  End
  Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
  If $Deskcurrent == 1 Then
  Begin
    Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    ChangeTitle 4 $Deskcurrent
    ChangeIcon 3 $aw0
    ChangeIcon 1 $w0
    ChangeIcon 5 $w0
    ChangeIcon 7 $w0
    ChangeIcon 9 $w0
    ChangeColorset 4 15
    ChangeColorset 2 28 
    ChangeColorset 6 28
    ChangeColorset 8 28 
    ChangeColorset 10 28 
  End
  Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    If $Deskcurrent == 2 Then
  Begin
    Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    ChangeTitle 6 $Deskcurrent
    ChangeIcon 5 $aw0
    ChangeIcon 1 $w0
    ChangeIcon 3 $w0
    ChangeIcon 7 $w0
    ChangeIcon 9 $w0
    ChangeColorset 6 15
    ChangeColorset 2 28 
    ChangeColorset 4 28
    ChangeColorset 8 28
    ChangeColorset 10 28
  End
  Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    If $Deskcurrent == 3 Then
  Begin
    Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    ChangeTitle 8 $Deskcurrent
    ChangeIcon 7 $aw0
    ChangeIcon 1 $w0
    ChangeIcon 3 $w0
    ChangeIcon 5 $w0
    ChangeIcon 9 $w0
    ChangeColorset 8 15
    ChangeColorset 2 28 
    ChangeColorset 4 28
    ChangeColorset 6 28
    ChangeColorset 10 28 
  End
  Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    If $Deskcurrent == 4 Then
  Begin
    Set $Deskcurrent = (GetOutput {exec wmctrl -d | grep '*' | cut -d ' ' -f1} 1 -1)
    ChangeTitle 10 $Deskcurrent
    ChangeIcon 9 $aw0
    ChangeIcon 1 $w0
    ChangeIcon 3 $w0
    ChangeIcon 5 $w0
    ChangeIcon 7 $w0
    ChangeColorset 8 28
    ChangeColorset 10 15
    ChangeColorset 2 28 
    ChangeColorset 4 28
    ChangeColorset 6 28
  End
 End
End
  
  
