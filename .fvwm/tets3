    ChangeColorset 22 11          
    ChangeColorset 23 11          
    ChangeColorset 24 11          
    ChangeColorset 25 28  
  End
  Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d'} 1 -1)
  Set $Desk66 = (GetOutput {echo 100%} 1 -1)
    If $Deskcurrent == $Desk66 Then
  Begin
    ChangeColorset 1 11
    ChangeColorset 2 11 
    ChangeColorset 3 11   
    ChangeColorset 4 11   
    ChangeColorset 5 11
    ChangeColorset 6 11   
    ChangeColorset 7 11
    ChangeColorset 8 11      
    ChangeColorset 9 11      
    ChangeColorset 10 11      
    ChangeColorset 11 11      
    ChangeColorset 12 11      
    ChangeColorset 13 11      
    ChangeColorset 14 11
    ChangeColorset 15 11          
