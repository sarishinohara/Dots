WindowTitle {notif}
WindowSize  20 20
Font "xft:Bebas Neue:style=Bold:size=10" 

Init
 Begin
  Set $Track = (GetOutput {exec wc -l < ~/.fvwm/memento_file} 1 -1)

  ChangeTitle 1 $Track

End

PeriodicTasks
 Begin 
  If (RemainderOfDiv (GetTime) 20)==0 Then
  Begin
  Set $Track = (GetOutput {exec wc -l < ~/.fvwm/memento_file} 1 -1)

   ChangeTitle 1 $Track

  End
End

Widget 1
Property
 Position 0 0
 Size 20 20
 Font "xft:Bebas Neue:style=Bold:size=10"
 Type ItemDraw
 Flags NoReliefString
 Title {}
Main
 Case message of
  SingleClic :
  Begin
  Do {Module FvwmButtons faa}              
  End
End

