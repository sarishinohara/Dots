WindowTitle {widget7}
WindowSize  160 161
WindowPosition -274 -85
Colorset 666
Font "xft:Noto Sans cjk kr:style=Bold:size=14" 

Init
 Begin
  Set $git = (GetOutput {echo GT } 1 -1)
  Set $homeDir = (GetOutput {echo $HOME} 1 -1) {/.fvwm/}
  Set $Icon = $homeDir {pixmaps/bubbles/111.png}
  Set $Icon2 = $homeDir {pixmaps/bubbles/222.png}
  Set $Icon3 = $homeDir {pixmaps/bubbles/333.png}
  Set $Icon4 = $homeDir {pixmaps/bubbles/444.png}
  Set $yt = (GetOutput {echo YT} 1 -1)
  Set $4C = (GetOutput {echo 4C} 1 -1)
  Set $redd = (GetOutput {echo RD} 1 -1)



  Set $tasksFile = $homeDir {memento_file}

  Set $num = {wc -l < ~/.fvwm/memento_file} 


  ChangeIcon 1 $Icon
  ChangeIcon 2 $Icon2
  ChangeIcon 3 $Icon3
  ChangeIcon 4 $Icon4
  ChangeTitle 5 $git
  ChangeTitle 6 $yt
  ChangeTitle 7 $4C
  ChangeTitle 8 $redd


End


