WindowTitle {tets6}
WindowSize  192 3
WindowPosition 300 500

Font "xft:Noto Sans cjk kr:style=Bold:size=14" 

Init
 Begin
  Set $Desk0 = (GetOutput {echo } 1 -1)
  Set $Desk1 = (GetOutput {echo } 1 -1)
  Set $Desk2 = (GetOutput {echo } 1 -1)
  Set $Desk3 = (GetOutput {echo } 1 -1)
  Set $Desk4 = (GetOutput {echo } 1 -1)
  Set $Desk5 = (GetOutput {echo } 1 -1)
  Set $Desk6 = (GetOutput {echo } 1 -1)
  Set $Desk7 = (GetOutput {echo } 1 -1)
  Set $Desk8 = (GetOutput {echo } 1 -1)
  Set $Desk9 = (GetOutput {echo } 1 -1)
  Set $Desk10 = (GetOutput {echo } 1 -1)
  Set $Desk11 = (GetOutput {echo } 1 -1)
  Set $Desk12 = (GetOutput {echo } 1 -1)
  Set $Desk13 = (GetOutput {echo } 1 -1)
  Set $Desk14 = (GetOutput {echo } 1 -1)  
  Set $homeDir = (GetOutput {echo $HOME} 1 -1) {/.fvwm/}
  Set $w0 = $homeDir {pixmaps/bubbles/w0.png}
  Set $aw0 = $homeDir {pixmaps/bubbles/m.png}


End

PeriodicTasks
## First Page
Begin
 If (RemainderOfDiv (GetTime) 1)==0 Then
 Begin
  Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=1 To 5 Do
   Begin
   ChangeColorset 1 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=6 To 11 Do
   Begin
   ChangeColorset 2 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=12 To 17 Do
   Begin
   ChangeColorset 3 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=17 To 22 Do
   Begin
   ChangeColorset 4 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=22 To 27 Do
   Begin
   ChangeColorset 5 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=27 To 33 Do
   Begin
   ChangeColorset 6 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=33 To 38 Do
   Begin
   ChangeColorset 7 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=38 To 43 Do
   Begin
   ChangeColorset 8 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=43 To 48 Do
   Begin
   ChangeColorset 9 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=48 To 53 Do
   Begin
   ChangeColorset 10 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=53 To 58 Do
   Begin
   ChangeColorset 11 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=58 To 63 Do
   Begin
   ChangeColorset 12 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=63 To 68 Do
   Begin
   ChangeColorset 13 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=68 To 73 Do
   Begin
   ChangeColorset 14 11
   End
   Set $Deskcurrent = (GetOutput {exec mpc | sed 's/.*(//;s/)//;2q;d' | rev | cut -c 2- | rev} 1 -1)
  For $Deskcurrent=73 To 78 Do
   Begin
   ChangeColorset 15 11
   End
 End
End
  

